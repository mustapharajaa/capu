# CapCut Automation Environment Configuration
# Copy this file to .env and update the values

# FFmpeg and yt-dlp paths (automatically configured by setup.js)
FFMPEG_PATH=# FFMPEG_PATH will use npm package fallback - comment out or remove the line above
YTDLP_PATH=./bin/yt-dlp.exe

# Port for the web server
PORT=3000

# Batch processor mode
batch-processor=true

# Google Sheets integration (optional)
# Method 1: Google Apps Script Web App (simple setup)
GOOGLE_SHEETS_URL=

# Method 2: Service Account (more secure and robust)
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_SPREADSHEET_ID=your-spreadsheet-id
GOOGLE_SHEET_NAME=Sheet1



